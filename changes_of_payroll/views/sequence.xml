<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

       <record id="seq_employee_request" model="ir.sequence">
            <field name="name">Purchase Requisition Order Location</field>
            <field name="code">employee.request</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for changes.payroll -->
        <record id="seq_changes_payroll" model="ir.sequence">
            <field name="name">changes.payroll</field>
            <field name="code">changes.payroll</field>
            <field name="prefix">FHA One-time changes/%(range_month)s/%(range_year)s/ </field>
            <field name="suffix"></field>
            <field name="padding">1</field>
            <field eval="True" name="use_date_range"/>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
            <field name="number_increment">1</field>
        </record>


    </data>
</odoo>